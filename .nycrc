{
  "all": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "**/*.test.js",
    "**/*.spec.js",
    "**/*.unit.js",
    "**/*.feature.js",
    "**/*.e2e.js",
    "**/__fixtures__/**",
    "src/index.js"
  ],
  "reporter": [
    "text",
    "text-summary",
    "lcov",
    "clover"
  ],
  "check-coverage": true,
  "watermarks": {
    "lines": [
      20,
      95
    ],
    "functions": [
      20,
      95
    ],
    "branches": [
      20,
      95
    ],
    "statements": [
      20,
      95
    ]
  }
}
